{
  "name": "hardhat-project",
  "scripts": {
    "node": "hardhat node",
    "help": "hardhat --help",
    "test": "hardhat test",
    "test:coverage": "hardhat test --coverage",
    "build": "hardhat compile",
    "size": "hardhat size-contracts",
    "deploy:sepolia": "hardhat run --network sepolia scripts/deploy.js",
    "verify:sepolia": "hardhat run --network sepolia scripts/verify.js",
    "export:sepolia": "hardhat export --network sepolia --address",
    "import:sepolia": "hardhat import --network sepolia --address",
    "deploy:ganache": "hardhat run --network ganache scripts/deploy.js",
    "import:ganache": "hardhat import --network ganache --address",
    "export:ganache": "hardhat export --network ganache --address",
    "prettier": "prettier --write contracts/**/*.sol"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@openzeppelin/contracts": "4.6.0",
    "chai": "^4.3.7",
    "chai-arrays": "^2.2.0",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.3.1",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.7.2",
    "hardhat": "^2.16.1",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^1.0.9",
    "md5": "^2.3.0",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.1.3",
    "solidity-coverage": "^0.8.4"
  },
  "resolutions": {
    "@solidity-parser/parser": "^0.14.0"
  },
  "dependencies": {
    "semver": "^7.5.3"
  }
}
