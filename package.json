{
  "name": "@dapplets/dapplet-registry",
  "version": "0.1.0",
  "description": "Ethereum-based Dapplet Registry",
  "main": "",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "truffle test",
    "migrate": "truffle migrate --skip-dry-run --reset",
    "migrate:localhost": "truffle migrate --network localhost --skip-dry-run --reset",
    "migrate:rinkeby": "truffle migrate --network rinkeby --skip-dry-run --reset",
    "migrate:goerli": "truffle migrate --network goerli --skip-dry-run --reset",
    "migrate:aurora": "truffle migrate --network aurora --skip-dry-run --reset",
    "build": "truffle compile"
  },
  "author": "Dapplets Team",
  "license": "MIT",
  "devDependencies": {
    "@truffle/hdwallet-provider": "^1.0.34",
    "eth-gas-reporter": "^0.2.17",
    "lodash": "^4.17.15",
    "truffle": "^5.1.22",
    "web3": "^1.2.8",
    "web3-provider-engine": "*"
  },
  "dependencies": {
    "semver": "^7.3.2"
  }
}
