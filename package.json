{
  "name": "hardhat-project",
  "scripts": {
    "node": "hardhat node",
    "help": "hardhat --help",
    "test": "hardhat test",
    "test:coverage": "hardhat test --coverage",
    "build": "hardhat compile",
    "size": "hardhat size-contracts",
    "deploy:goerli": "hardhat run --network goerli scripts/deploy.js",
    "deploy:rinkeby": "hardhat run --network rinkeby scripts/deploy.js",
    "deploy:ganache": "hardhat run --network ganache scripts/deploy.js",
    "export:goerli": "hardhat export --network goerli --address",
    "export:ganache": "hardhat export --network ganache --address",
    "import:goerli": "hardhat import --network goerli --address",
    "import:ganache": "hardhat import --network ganache --address",
    "prettier": "prettier --write contracts/**/*.sol"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.6.0",
    "chai": "^4.3.6",
    "chai-arrays": "^2.2.0",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^16.0.1",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.5",
    "hardhat": "^2.13.0",
    "hardhat-contract-sizer": "^2.5.1",
    "hardhat-gas-reporter": "^1.0.8",
    "md5": "^2.3.0",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "^1.0.0-dev.23",
    "solidity-coverage": "^0.7.21"
  },
  "resolutions": {
    "@solidity-parser/parser": "^0.14.0"
  },
  "dependencies": {
    "semver": "^7.3.7"
  }
}
